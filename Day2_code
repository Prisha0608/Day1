#include <iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int e=0;
    int sum=0;
    while(n>0){
        e=n%10;
        sum+=e;
        n/=10;
    }
    cout<<sum;
    return 0;
}

int main(){
    int n;
    cin>>n;
    int e=0;
    int r=0;
    while(n>0){
        e=n%10;
        r=r*10+e;
        n/=10;
    }
    cout<<r;
}

int main(){
    int n;
    cin>>n;
    for(int i=0;i<=n;i++){
        cout<<i<<" ";
    }
}

 


int main(){
    int n;
    cin>>n;
    int fact=1;
    for(int i=2;i<=n;i++){
        fact*=i;
    }
    cout<<fact;
}


int main(){
    int n;
    cin>>n;
    int e=0;
    int r=0;
    for(int i=n;i>0;i/=10){
       e=i%10;
       r=r*10+e;
       
    }
    cout<<r;
}
